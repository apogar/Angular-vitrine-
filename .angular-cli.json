{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "angular-project"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/semantic-ui/dist/semantic.min.css",
        "../node_modules/semantic-ui/dist/components/card.min.css",
        "../node_modules/semantic-ui/dist/components/rating.min.css",
        "../node_modules/semantic-ui/dist/components/popup.min.css",
        "../node_modules/semantic-ui/dist/components/search.min.css",
        "../node_modules/semantic-ui/dist/components/transition.min.css",
        "../node_modules/semantic-ui/dist/components/accordion.min.css",
        "../node_modules/semantic-ui/dist/components/dropdown.min.css",
        "../node_modules/semantic-ui/dist/components/tab.min.css",
        "../node_modules/semantic-ui/dist/components/sticky.min.css",
        "../node_modules/semantic-ui/dist/components/rail.min.css",
        "../node_modules/semantic-ui/dist/components/label.min.css",
        "../node_modules/semantic-ui/dist/components/table.min.css",
        "styles.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/semantic-ui/dist/semantic.min.js",
        "../node_modules/semantic-ui/dist/components/rating.min.js",
        "../node_modules/semantic-ui/dist/components/popup.min.js",
        "../node_modules/semantic-ui/dist/components/search.min.js",
        "../node_modules/semantic-ui/dist/components/transition.min.js",
        "../node_modules/semantic-ui/dist/components/accordion.min.js",
        "../node_modules/semantic-ui/dist/components/dropdown.min.js",
        "../node_modules/semantic-ui/dist/components/tab.min.js",
        "../node_modules/semantic-ui/dist/components/sticky.min.js",
        "main.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
